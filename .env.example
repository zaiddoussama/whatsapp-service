# Node.js server port
PORT=3000

# Spring Boot backend URL for webhooks
SPRING_BOOT_URL=http://localhost:8080/api

# API key for authentication (must match Spring Boot)
API_KEY=your-api-key-here

# Webhook API key (for Spring Boot to validate incoming webhooks)
WEBHOOK_API_KEY=your-webhook-key-here

# Node environment
NODE_ENV=development
